        <method name="clearServerConfig"/>
        <method name="saveServerAuthData">
            <arg direction="in" type="s" name="login" />
            <arg direction="in" type="s" name="password" />
        </method>
        <method name="initDevice">
            <arg direction="in" type="s" name="device" />
            <arg direction="in" type="s" name="device_name" />
            <arg direction="in" type="s" name="device_latin" />
            <arg direction="in" type="s" name="device_desc" />
        </method>

        <method name="setControlState">
            <arg direction="in" type="u" name="section_id" />
            <arg direction="in" type="u" name="device_type" />
            <arg direction="in" type="{bv}" name="state" />
            <annotation name="org.qtproject.QtDBus.QtTypeName.In2" value="const Dai::Variant&amp;"/>
        </method>
        <method name="setMode">
            <arg direction="out" type="b" />
            <arg direction="in" type="u" name="section_id" />
            <arg direction="in" type="u" name="group_id" />
            <arg direction="in" type="u" name="mode" />
        </method>
        <signal name="modeChanged">
            <arg direction="out" type="u" name="mode_id" />
            <arg direction="out" type="u" name="group_id" />
        </signal>
        <signal name="groupStatusChanged">
            <arg direction="out" type="u" name="group_id" />
            <arg direction="out" type="u" name="status" />
        </signal>
        <signal name="paramValuesChanged">
            <arg direction="out" type="{us}" name="pack" />
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="const Dai::ParamValuesPack&amp;"/>
        </signal>

        <method name="setDayTime">
            <arg direction="out" type="b" />
            <arg direction="in" type="u" name="section_id" />
            <arg direction="in" type="u" name="dayStartSecs" />
            <arg direction="in" type="u" name="dayEndSecs" />
        </method>

        <signal name="started"/>

        <signal name="change">
            <arg direction="out" type="{uulvv}" name="item" />
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="const Dai::ValuePackItem&amp;"/>
            <arg direction="out" type="b" name="immediately" />
        </signal>

